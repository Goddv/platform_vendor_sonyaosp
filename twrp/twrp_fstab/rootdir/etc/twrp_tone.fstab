# mount point    fstype    device                                     [device2]              fstype2
/boot            emmc      /dev/block/bootdevice/by-name/boot
/system          ext4      /dev/block/bootdevice/by-name/system
/system_image    emmc      /dev/block/bootdevice/by-name/system
/odm             ext4      /dev/block/bootdevice/by-name/oem                                 flags=backup=1
/cache           ext4      /dev/block/bootdevice/by-name/cache
/data            ext4      /dev/block/bootdevice/by-name/userdata
/recovery        emmc      /dev/block/bootdevice/by-name/FOTAKernel                          flags=backup=1

# Removable storage
/sdcard1         auto      /dev/block/mmcblk1p1                       /dev/block/mmcblk1     flags=display="Micro SDcard";storage;wipeingui;removable
